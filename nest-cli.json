{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "/public/images/images_url/*.png",
        "outDir": "./dist/public/images/images_url/*.png"
      },
      {
        "include": "/public/images/images_url_temp/*.png",
        "outDir": "./dist/public/images/images_url_temp/*.png"
      }
    ],
    "deleteOutDir": true
  }
}
